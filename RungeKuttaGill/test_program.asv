%%	calculate rigid body motion %	clear %初期化のコマンド	const;%global変定のmファイル    % このｍファイルの変数の定義	dth_ = 0.01;       %刻み時間	nde_ = 10;	smt_ = 10;          %終了時刻	svtime = 0;	cntime = 0;	dptime = 0;	x_ = zeros(nde_,1);    dx_ = zeros(nde_,1);    qx_ = zeros(nde_,1);%%	simulation%	nsave = 2;	ncont = 2;	ndisp = 100;    	preset;	% initialize routine %    	t_ = 0;	ts = [];%空っぽの行列     xs = [];    	while(1)%うんどうほうていしきをるげくったほうで０．０１		rkg('motion');% ルンゲクッタ法でmotionファイルの運動方程式を解く        		if t_ >= svtime  %x_,t_を記録			xs = [xs, x_];            ts = [ts; t_];			svtime = svtime + dth_*nsave;	  	end% 	  	if t_ >= cntime%             dgtal;%             cntime = cntime + dth_*ncont;% 	  	end	  	if t_ >= dptime %表示			disp(['time = ',num2str(t_)]);            dptime = dptime + dth_*ndisp;	  	end		if t_ >= smt_			break		end	end    simplot;%グラフを作成